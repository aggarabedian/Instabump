<div class="navbar is-inline-flex is-transparent">
  <div class="navbar-brand">
      <a class="navbar-item">
          <img src="https://bulma.io/images/bulma-logo.png" width="112" height="28" alt="Bulma">
      </a>
  </div>

</div>

  <!-- this will add a link to the new route -->
  <div class="navbar-menu navbar-end">
    
    <% if (typeof user !== 'undefined') { %>
      <a href="/users/" class="navbar-item"><%= user.username %></a>
    <% } %>

    <% if (typeof routes !== 'undefined') { %> <% routes.forEach((route) => { %>
    <a class="navbar-item" href="<%= route.href %>"> <%= route.title %> </a>
    <% }) %> <% } %>
  </div>

